  "description": "",
  "main": "index.js",
  "scripts": {
    "server": "cross-env BABEL_ENV=build webpack --env.c --progress && cross-env BABEL_ENV=build babel-node server.js",
    "build:prod": "cross-env NODE_ENV=production BABEL_ENV=build webpack --env.p --env.c",
    "build:vis": "cross-env NODE_ENV=production BABEL_ENV=build webpack --env.visualize --env.p --env.c",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "generate-localization-keys": "cross-env BABEL_ENV=build babel-node -- ./scripts/localization",
    "generate-localization-keys:watch": "cross-env BABEL_ENV=build babel-node -- ./scripts/localization/watch",
    "generate-store-types": "cross-env BABEL_ENV=build babel-node -- ./scripts/store-types/store-types",
    "generate-store-types:watch": "cross-env BABEL_ENV=build babel-node -- ./scripts/store-types/watch",
    "generate-swagger-client": "npm run generate-flow-types && npm run generate-flow-client && npm run generate-enums",
    "generate-enums": "cross-env BABEL_ENV=build babel-node -- ./scripts/swagger-to-flow/generate-enums http://localhost:53555/swagger/v1/swagger.json",
    "generate-flow-client": "cross-env BABEL_ENV=build babel-node -- ./scripts/swagger-to-flow/swagger-to-api-client http://localhost:53555/swagger/v1/swagger.json",
    "generate-flow-types": "cross-env BABEL_ENV=build babel-node -- ./scripts/swagger-to-flow/swagger-to-flow http://localhost:53555/swagger/v1/swagger.json",
    "flow": "flow check",
    "lint": "eslint --ext .jsx,.js src/ --fix"
  },
  "jest": {
    "globals": {
      "HOST": "HOST"
    },
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^mock-setup(/?.*)$": "<rootDir>/mock-setup/$1",
      "^~(/?.*)$": "<rootDir>/src/$1",
      "^common-components(/?.*)$": "<rootDir>/src/components/$1"
    }
  },
  "browserslist": [
    "last 2 versions"
  ],
  "pre-push": [
    "test"
  ],
