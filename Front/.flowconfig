[options]
esproposal.decorators=ignore
module.name_mapper='^~\(\/?.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^common-components\(\/?.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^mock-setup\(\/?.*\)$' -> '<PROJECT_ROOT>/mock-setup/\1'
module.file_ext=.js
module.file_ext=.json
module.file_ext=.jsx
module.file_ext=.css
module.file_ext=.scss
module.file_ext=.svg
module.name_mapper.extension='css' -> 'identity-obj-proxy'
module.name_mapper.extension='scss' -> 'identity-obj-proxy'
module.name_mapper.extension='svg' -> 'identity-obj-proxy'
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue

[libs]
src/types/npm/

[ignore]
.*/node_modules/webpack-cli/.*
.*/scripts/babel-transforms/.*